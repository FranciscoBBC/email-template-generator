{
  "name": "email-template-generator",
  "version": "1.0.0",
  "description": "dynamic email template generator",
  "main": "server.ts",
  "scripts": {
    "start": "node ./src/server.ts",
    "dev": "nodemon ./src/server.ts",
    "test": "jest",
    "lint:fix": "lint-staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && yarn test"
    }
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "keywords": [
    "email",
    "template",
    "generator"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FranciscoBBC/email-template-generator.git"
  },
  "author": "Francisco Castro",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/FranciscoBBC/email-template-generator/issues"
  },
  "homepage": "https://github.com/FranciscoBBC/email-template-generator#readme",
  "devDependencies": {
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.25",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "eslint": "^7.19.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^6.14.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "mjml": "^4.8.1"
  }
}
